Host * !bitbucket.org
  Compression yes
  ServerAliveInterval 240
  ControlMaster auto
  # ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPath ~/.ssh/sockets/%C.sock
  ControlPersist 20m
  VisualHostKey yes
  GSSAPIAuthentication no

